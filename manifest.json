{
    "name": "History Extension",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
        "default_title": "History Extension",
        "default_popup": "/public/index.html"
    },
    "background": {
        "scripts":["/background.js"]
    },
    "permissions": [
        "contextMenus",
        "storage",
        "notifications"
      ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["/scripts/content_script.js"]
        }
    ]
}